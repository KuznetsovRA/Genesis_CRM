{
  "version": 2,
  "builds": [
    {
      "src": "api/src/main.ts",
      "use": "@vercel/node"
    },
    {
      "src": "client/package.json",
      "use": "@vercel/static-build"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "api/src/main.ts"
    },
    {
      "src": "/(.*)",
      "dest": "/client/$1"
    }
  ]
}

